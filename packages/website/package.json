{
  "name": "soybean-admin",
  "version": "0.9.9",
  "type": "commonjs",
  "description": "A fresh and elegant admin template, based on Vue3、Vite3、TypeScript、NaiveUI and UnoCSS. 一个基于Vue3、Vite3、TypeScript、NaiveUI and UnoCSS的清新优雅的中后台模版。",
  "author": {
    "name": "Soybean",
    "email": "honghuangdc@gmail.com",
    "url": "https://github.com/honghuangdc"
  },
  "license": "MIT",
  "homepage": "https://github.com/honghuangdc/soybean-admin",
  "repository": {
    "url": "https://github.com/honghuangdc/soybean-admin.git"
  },
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-admin/issues"
  },
  "keywords": [
    "Vue",
    "Vue3",
    "admin",
    "admin-template",
    "vue-admin",
    "vue-admin-template",
    "Vite3",
    "Vite",
    "vite-admin",
    "TypeScript",
    "TS",
    "NaiveUI",
    "naive-ui",
    "naive-admin",
    "NaiveUI-Admin",
    "naive-ui-admin",
    "UnoCSS"
  ],
  "scripts": {
    "ssr": "cross-env VITE_SERVICE_ENV=prod vite",
    "dev": "cross-env VITE_SERVICE_ENV=dev vite",
    "dev:test": "cross-env VITE_SERVICE_ENV=test vite",
    "dev:prod": "cross-env VITE_SERVICE_ENV=prod vite",
    "build": "npm run typecheck && cross-env VITE_SERVICE_ENV=prod vite build --ssrManifest",
    "build:dev": "npm run typecheck && cross-env VITE_SERVICE_ENV=dev vite build",
    "build:test": "npm run typecheck && cross-env VITE_SERVICE_ENV=test vite build",
    "build:vercel": "cross-env VITE_HASH_ROUTE=Y VITE_VERCEL=Y vite build",
    "preview": "cross-env VITE_SERVICE_ENV=prod vite preview -m production",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --fix --ext .js,.jsx,.mjs,.json,.ts,.tsx,.vue",
    "formatx": "soy prettier-format",
    "format": "run-s format:*",
    "format:js": " prettier --write \"src/**/*.(ts|js|vue|css|less|scss)\"  ",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy update-pkg",
    "tsx": "tsx",
    "logo": "tsx ./scripts/logo.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version"
  },
  "dependencies": {
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^4.2.10",
    "@better-scroll/core": "^2.5.1",
    "@soybeanjs/vue-materials": "^0.1.9",
    "@vueuse/core": "^10.1.0",
    "axios": "0.27.2",
    "clipboard": "^2.0.11",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "2.34.3",
    "pinia": "^2.0.35",
    "print-js": "^1.6.0",
    "qs": "^6.11.1",
    "swiper": "^9.2.4",
    "ua-parser-js": "^1.0.35",
    "vditor": "^3.9.2",
    "vite-plugin-ssr": "^0.4.122",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "wangeditor": "^4.7.15",
    "xgplayer": "^3.0.1"
  },
  "devDependencies": {
    "@ai-lion/vite-plugin-vue-page-route": "^1.0.2",
    "@amap/amap-jsapi-types": "^0.0.13",
    "@iconify/json": "^2.2.55",
    "@iconify/vue": "^4.1.1",
    "@soybeanjs/cli": "^0.1.8",
    "@soybeanjs/vite-plugin-vue-page-route": "^0.0.5",
    "@types/bmapgl": "^0.0.7",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "18.16.1",
    "@types/qs": "^6.9.7",
    "@types/ua-parser-js": "^0.7.36",
    "@unocss/preset-uno": "^0.51.8",
    "@unocss/transformer-directives": "^0.51.8",
    "@unocss/vite": "^0.51.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "conventional-changelog": "^3.1.25",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-soybeanjs": "^0.3.3",
    "lint-staged": "12.5.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "standard-version": "^9.5.0",
    "tsx": "^3.12.6",
    "typescript": "5.0.4",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "0.24.1",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "^4.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-static-copy": "^0.14.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.4.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "mockjs@1.1.0": "patches/mockjs@1.1.0.patch"
    }
  },
  "lint-staged": {
    "*.{js,jsx,mjs,json,ts,tsx,vue}": "eslint . --fix"
  },
  "imports": {
    "~/*": "./*",
    "@/*": "./src/*",
    "vue-i18n": "vue-i18n/dist/vue-i18n.cjs.js"
  }
}
